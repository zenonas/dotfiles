{
  "Private method": {
    "prefix": "pdef",
    "body": [
      "private def $1",
      "  $2",
      "end"
    ]
  },

  "Lazy Load": {
    "prefix": "ll",
    "body": [ "lazy_load(:$1) { $2 }" ]
  },
  "Lazy Entity": {
    "prefix": "le",
    "body": [ "lazy_entity(:$1) { $2 }" ]
  },
  "Lazy Entities": {
    "prefix": "les",
    "body": [ "lazy_entities(:$1) { $2 }" ]
  },

  "Log - debug": {
    "prefix": "logd",
    "body": [ "log.debug { $1 }" ]
  },
  "Log - info": {
    "prefix": "logi",
    "body": [ "log.info { $1 }" ]
  },
  "Log - warn": {
    "prefix": "logw",
    "body": [ "log.warn { $1 }" ]
  },
  "Log - error": {
    "prefix": "loge",
    "body": [ "log.error { $1 }" ]
  },
  "Log - fatal": {
    "prefix": "logf",
    "body": [ "log.fatal { $1 }" ]
  },

  "Entity Store - save": {
    "prefix": "es",
    "body": [ "entity_store.save($1)" ]
  },
  "Entity Store - tap save": {
    "prefix": "ets",
    "body": [ ".tap { |$1| entity_store.save($1) }" ]
  }
}
